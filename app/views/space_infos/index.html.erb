<p id="notice"><%= notice %></p>

<h1>Space Infos</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Price per hour</th>
      <th>Ot</th>
      <th>Ct</th>
      <th>Tel</th>
      <th>Photo</th>
      <th>Parking</th>
      <th>Smoking</th>
      <th>Projector</th>
      <th>Wifi</th>
      <th>Printer</th>
      <th>Consent</th>
      <th>Payment</th>
      <th>Place</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @space_infos.each do |space_info| %>
      <tr>
        <td><%= space_info.name %></td>
        <td><%= space_info.address %></td>
        <td><%= space_info.price_per_hour %></td>
        <td><%= space_info.ot %></td>
        <td><%= space_info.ct %></td>
        <td><%= space_info.tel %></td>
        <td><%= space_info.photo %></td>
        <td><%= space_info.parking %></td>
        <td><%= space_info.smoking %></td>
        <td><%= space_info.projector %></td>
        <td><%= space_info.wifi %></td>
        <td><%= space_info.printer %></td>
        <td><%= space_info.consent %></td>
        <td><%= space_info.payment %></td>
        <td><%= space_info.place %></td>
        <td><%= link_to 'Show', space_info %></td>
        <td><%= link_to 'Edit', edit_space_info_path(space_info) %></td>
        <td><%= link_to 'Destroy', space_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Space Info', new_space_info_path %>
